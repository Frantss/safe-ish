{
  "name": "safe-ish",
  "private": false,
  "version": "0.0.1",
  "description": "Safe-ish error handling.",
  "keywords": ["safe", "error", "handling", "typescript"],
  "files": ["dist"],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./prefixed": {
      "import": {
        "types": "./dist/prefixed.d.ts",
        "default": "./dist/prefixed.js"
      },
      "require": {
        "types": "./dist/prefixed.d.cts",
        "default": "./dist/prefixed.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "check-exports": "attw --pack . --profile node16",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepublishOnly": "bun run lint && bun run build && bun run check-exports",
    "version": "changeset",
    "release": "changeset version && changeset publish",
    "test": "vitest",
    "test:dev": "vitest --watch"
  },
  "bugs": {
    "url": "https://github.com/Frantss/safe-ish/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Frantss/safe-ish"
  },
  "author": "Frantss <frantss.bongiovanni@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.29.2",
    "@types/bun": "latest",
    "tsdown": "^0.9.9",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2"
  }
}
